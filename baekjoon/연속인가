#include <iostream>

using namespace std;


int main()
{
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	long long n;
	cin >> n;
	long long arr[4];
	for (int i = 0; i < 4; i++)
		cin >> arr[i];
	if (arr[0] * n + arr[1] == arr[2] * n + arr[3])
		cout << "Yes" << ' ' << arr[0] * n + arr[1];
	else
		cout << "No";
	return 0;
}